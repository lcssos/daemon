<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.martin.upload.repository.FileMapper" >

    <cache type="org.mybatis.caches.redis.RedisCache" />

    <resultMap id="BaseResultMap" type="cn.martin.upload.entity.File">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="chunks" property="chunks" jdbcType="INTEGER" />
        <result column="path" property="path" jdbcType="VARCHAR" />
        <result column="status" property="status" />
        <result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
    </resultMap>

    <sql id="Base_Column_List">
        id,chunks,path,status,createtime
    </sql>

    <select id="selectByPrimaryKey" parameterType="long" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List" />
        from t_file where id=#{id}
    </select>




</mapper>